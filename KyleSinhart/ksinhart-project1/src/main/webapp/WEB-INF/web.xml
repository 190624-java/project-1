<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>project1</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>EmployeeListServlet</display-name>
    <servlet-name>EmployeeListServlet</servlet-name>
    <servlet-class>com.revature.project1.servlets.EmployeeListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EmployeeListServlet</servlet-name>
    <url-pattern>/app/emplist</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ViewReimbursementServlet</display-name>
    <servlet-name>ViewReimbursementServlet</servlet-name>
    <servlet-class>com.revature.project1.servlets.ViewReimbursementServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewReimbursementServlet</servlet-name>
    <url-pattern>/app/relist</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ReceiptListServlet</display-name>
    <servlet-name>ReceiptListServlet</servlet-name>
    <servlet-class>com.revature.project1.servlets.ReceiptListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ReceiptListServlet</servlet-name>
    <url-pattern>/app/reclist</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LoginServlet</display-name>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.revature.project1.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/app/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>RegistrationServlet</display-name>
    <servlet-name>RegistrationServlet</servlet-name>
    <servlet-class>com.revature.project1.servlets.RegistrationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegistrationServlet</servlet-name>
    <url-pattern>/app/registration</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>InformationChangeServlet</display-name>
    <servlet-name>InformationChangeServlet</servlet-name>
    <servlet-class>com.revature.project1.servlets.InformationChangeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InformationChangeServlet</servlet-name>
    <url-pattern>/app/info</url-pattern>
  </servlet-mapping>
  
  <session-config>
  	<!-- 5 minute session timeout -->
  	<session-timeout>5</session-timeout>
  </session-config>
</web-app>